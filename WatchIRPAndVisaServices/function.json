{
  "disabled": false,
  "bindings": [
    {
      "type": "timerTrigger",
      "name": "Timer",
      "schedule": "*/30 * * * * *",
      "direction": "in"
    },
    {
      "name": "lastAppointments",
      "type": "documentDB",
      "databaseName": "GNIBIRPAndVisaAppointment",
      "collectionName": "Appointments",
      "sqlQuery": "SELECT Top 2 * FROM c ORDER BY c._ts DESC",
      "connection": "CosmosDBConnection",
      "direction": "in"
    },
    {
      "name": "$return",
      "type": "documentDB",
      "databaseName": "GNIBIRPAndVisaAppointment",
      "collectionName": "Appointments",
      "createIfNotExists": true,
      "connection": "CosmosDBConnection",
      "direction": "out"
    }
  ]
}